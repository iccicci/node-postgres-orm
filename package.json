{
  "name": "pgo",
  "version": "0.1.12",
  "description": "PostgreSQL dedicated ORM with automatic schema sync, table inheritance and much more",
  "main": "lib/pgo.js",
  "scripts": {
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive test",
    "eslint": "./node_modules/.bin/eslint lib/*js test/*js",
    "npmignore": "echo '.gitignore\\n.travis.yml\\ndoc*\\ntest\\npgo.png' > .npmignore ; cat .gitignore >> .npmignore",
    "t": "./node_modules/.bin/_mocha test",
    "test": "npm run npmignore && npm run eslint && npm run coverage"
  },
  "bugs": "https://github.com/iccicci/node-postgres-orm/issues",
  "repository": "https://github.com/iccicci/node-postgres-orm",
  "keywords": [
    "database",
    "postgres",
    "orm"
  ],
  "author": "Daniele Ricci <daniele.icc@gmail.com> (https://github.com/iccicci)",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "eslint": "3.13.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "mocha-istanbul": "0.3.0"
  },
  "dependencies": {
    "pg": "6.1.2"
  }
}
